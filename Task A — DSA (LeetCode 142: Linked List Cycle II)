# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def detectCycle(self, head: Optional[ListNode]) -> Optional[ListNode]:

        #floyds algorithm
        slow = head
        fast = head
        # Find the cycle where slow is equal to fast
        while fast and fast.next:
            slow = slow.next
            fast = fast.next.next

            if slow == fast:
                slow = head
                # resetting slow to head to get the cycle begin 
                while slow!=fast:
                    slow = slow.next
                    fast = fast.next
                return slow
        
